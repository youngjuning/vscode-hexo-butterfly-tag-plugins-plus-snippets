{
  "Insert Text": {
    "prefix": [">text"],
    "body": ["{% ${1|u,emp,wavy,del,kbd,psw|} $1 %}"],
    "description": "Insert Text."
  },
  "Insert Span": {
    "prefix": [">span"],
    "body": [
      "{% span ${2|logo,code|} ${3|red,yellow,green,cyan,blue,gray|} ${4|small,h4,h3,h2,h1,large,huge,ultra|} ${5|left,center,right|} $1%}"
    ],
    "description": "Insert Span."
  },
  "Insert Paragraph": {
    "prefix": [">p"],
    "body": [
      "{% p ${2|logo,code|} ${3|red,yellow,green,cyan,blue,gray|} ${4|small,h4,h3,h2,h1,large,huge,ultra|} ${5|left,center,right|} $1%}"
    ],
    "description": "Insert Paragraph."
  },
  "Insert Note": {
    "prefix": [">note"],
    "body": [
      "{% note ${1|default,primary,success,info,warning,danger,color|} ${2|simple,modern,flat,disabled|} %}",
      "$0",
      "{% endnote %}"
    ],
    "description": "Insert Note."
  },
  "Insert Tip": {
    "prefix": [">tip"],
    "body": [
      "{% tip ${1|success,error,warning,bolt,ban,home,sync,cogs,key,bell|} %}",
      "$0",
      "{% endnote %}"
    ],
    "description": "Insert Tip."
  },
  "Insert Animate Tip": {
    "prefix": ">tipAnimate",
    "body": [
      "{% tip ${1|success,error,warning,bolt,ban,home,sync,cogs,key,bell|} ${2|faa-wrench,faa-ring,faa-horizontal,faa-vertical,faa-flash,faa-bounce,faa-spin,faa-tada,faa-pulse,faa-shake,faa-passing,faa-passing-reverse,faa-burst,faa-falling,faa-rising,faa-parent|} ${3|animated,animated-hover|} ${4|faa-fast,faa-slow|} %}",
      "$0",
      "{% endtip %}"
    ],
    "description": "Insert Animate Tip"
  },
  "Insert Checkbox": {
    "prefix": ">checkbox",
    "body": "{% checkbox ${1|plus,minus,times|} ${2|red,green,yellow,cyan,blue|} ${3:checked}, ${4:text} %}",
    "description": "Insert Checkbox."
  },
  "Insert Radio": {
    "prefix": ">radio",
    "body": "{% radio ${2|red,green,yellow,cyan,blue|} ${3:checked}, ${4:text} %}",
    "description": "Insert Radio."
  },
  "Insert Timeline": {
    "prefix": ">timeline",
    "body": [
      "{% timeline ${1:title} ${2|default,blue,pink,red,purple,orange,green|} %}",
      "$0",
      "{% endtimeline %}"
    ],
    "description": "Insert Timeline"
  },
  "Insert Timeline Item": {
    "prefix": ">timelineItem",
    "body": ["<!-- timeline ${1:title} -->", "$0", "<!-- endtimeline -->"],
    "description": "Insert Timeline Item"
  },
  "Insert Butterfly Link": {
    "prefix": [">linkB"],
    "body": ["{% link ${1:title}, ${2:url}, ${3:imgUrl(option)} %}"],
    "description": "Insert Butterfly Link."
  },
  "Insert btns": {
    "prefix": [">btns"],
    "body": [
      "{% btns ${1|rounded,circle|} ${2|width,fill|} ${3|center,around|} ${4|grid2,grid3,grid4,grid5|} %}",
      "$0",
      "{% endbtns %}"
    ],
    "description": "Insert btns."
  },
  "Insert btns cell": {
    "prefix": [">btnsCell"],
    "body": ["{% cell ${1:title}, ${2:link}, ${3:iconOrImg} %}"],
    "description": "Insert btns cell."
  },
  "Insert Butterfly Button": {
    "prefix": [">btn"],
    "body": "{% btn ${1:link}, ${2:title}, ${3:icon}, ${4|outline,block|} ${5|blue,pink,red,purple,orange,green|} ${6:larger} %}",
    "description": "Insert Butterfly Button."
  },
  "Insert Butterfly Mermaid": {
    "prefix": [">mermaid"],
    "body": [
      "{% mermaid %}$0{% endmermaid %}"
    ],
    "description": "Insert Butterfly Mermaid."
  },
  "Insert Hide": {
    "prefix": [">hide"],
    "body": [
      "{% ${1|hideToggle,hideBlock,hideInline|} $2 %}",
      "$0",
      "{% end$1 %}"
    ],
    "description": "Insert Hide."
  },
  "Insert Folding": {
    "prefix": [">folding"],
    "body": [
      "{% folding ${1|blue,cyan,green,yellow,red|} ${2:open}, ${3:title} %}",
      "$0",
      "{% endfolding %}"
    ],
    "description": "Insert Folding."
  },
  "Insert Inline Image": {
    "prefix": ">inlineimage",
    "body": ["{% inlineimage ${1:link}, height=${2:option} %}"],
    "description": "Insert Inline Image"
  },
  "Insert Image": {
    "prefix": ">image",
    "body": "{% image ${0:url}, width=${1:100px}, height=${2:100px}, alt=${3:butterfly} bg=${4:#f2f2f2} %}",
    "description": "Insert Image"
  },
  "Insert Gallery Group": {
    "prefix": ">galleryGroup",
    "body": [
      "<div class=\"gallery-group-main\">",
      "{% galleryGroup ${1:name} ${2:description} ${3:link} ${imgUrl} %}",
      "</div>"
    ],
    "description": "Insert Gallery Group"
  },
  "Insert Gallery": {
    "prefix": ">gallery",
    "body": ["{% gallery %}", "${1:markdown 图片格式}", "{% endgallery %}"],
    "description": "Insert Gallery"
  },
  "Insert Tabs": {
    "prefix": ">tabs",
    "body": ["{% tabs ${1:Unique Name} %}", "$0", "{% endtabs %}"],
    "description": "Insert Tabs"
  },
  "Insert Tab": {
    "prefix": ">tab",
    "body": [
      "<!-- tab ${1:Tab caption}${2:@}${3:icon} -->",
      "$0",
      "<!-- endtab -->"
    ],
    "description": "Insert Tab"
  }
}
